{
  "name": "guud",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build.loc && npm run build.main",
    "build.loc": "ng build --localize",
    "build.main": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build.custom": "ng build --localize && npm run build.reloacate && npm run build.clean",
    "build.reloacate": "xcopy /e /h \"dist/en\" \"dist\"",
    "build.clean": "rmdir /s /q \"dist/en/\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "@sendgrid/mail": "^7.7.0",
    "bootstrap": "^5.2.3",
    "jquery": "^3.6.2",
    "jsonpack": "^1.1.5",
    "path-browserify": "^1.0.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.2",
    "@angular/cli": "~14.0.2",
    "@angular/compiler-cli": "^14.0.0",
    "@angular/localize": "^14.3.0",
    "@types/jasmine": "~4.0.0",
    "@types/jquery": "^3.5.14",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.7.2"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  }
}
